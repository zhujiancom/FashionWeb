package com.v2.po;

import java.util.List;

public class FooterPO {
    private String aboutus_EN;
    private String aboutus_CH;
    private String legalstmt_EN;
    private String legalstmt_CH;
    private List<Link> links;
    private Link sitemap;
    
    
    public FooterPO() {
        super();
        // TODO Auto-generated constructor stub
    }

    public String getAboutus_EN() {
        return aboutus_EN;
    }


    public void setAboutus_EN(String aboutus_EN) {
        this.aboutus_EN = aboutus_EN;
    }


    public String getAboutus_CH() {
        return aboutus_CH;
    }


    public void setAboutus_CH(String aboutus_CH) {
        this.aboutus_CH = aboutus_CH;
    }


    public String getLegalstmt_EN() {
        return legalstmt_EN;
    }


    public void setLegalstmt_EN(String legalstmt_EN) {
        this.legalstmt_EN = legalstmt_EN;
    }


    public String getLegalstmt_CH() {
        return legalstmt_CH;
    }


    public void setLegalstmt_CH(String legalstmt_CH) {
        this.legalstmt_CH = legalstmt_CH;
    }


    public List<Link> getLinks() {
        return links;
    }


    public void setLinks(List<Link> links) {
        this.links = links;
    }

    public Link getSitemap() {
        return sitemap;
    }

    public void setSitemap(Link sitemap) {
        this.sitemap = sitemap;
    }

    public static class Link{
        private String href;
        private String name_EN;
        private String name_CH;
        
        public Link() {
            super();
            // TODO Auto-generated constructor stub
        }
        public Link(String href) {
            super();
            this.href = href;
        }
        public Link(String href, String name_EN, String name_CH) {
            super();
            this.href = href;
            this.name_EN = name_EN;
            this.name_CH = name_CH;
        }
        public String getHref() {
            return href;
        }
        public void setHref(String href) {
            this.href = href;
        }
        public String getName_EN() {
            return name_EN;
        }
        public void setName_EN(String name_EN) {
            this.name_EN = name_EN;
        }
        public String getName_CH() {
            return name_CH;
        }
        public void setName_CH(String name_CH) {
            this.name_CH = name_CH;
        }
        @Override
        public String toString() {
            return "Link [href=" + href + ", name_EN=" + name_EN + ", name_CH=" + name_CH + "]";
        }
        
    }

    @Override
    public String toString() {
        return "FooterPO [aboutus_EN=" + aboutus_EN + ", aboutus_CH=" + aboutus_CH + ", legalstmt_EN=" + legalstmt_EN
                + ", legalstmt_CH=" + legalstmt_CH + ", links=" + links + ", sitemap=" + sitemap + "]";
    }
}
